{
  "manifest_version": 2,

  "name": "wishList",
  "description": "creates a wish list",
  "version": "1.0",

  "browser_action": {
    "default_icon": "hk.png",
    "default_popup": "popup.html"
    // "default_title": "Click here!"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["bower_components/angular/angular.js", "js/popup.js", "js/url.js", "js/storage.js", "js/jquery-1.12.0.min.js"],
      "run_at": "document_start"
    }
  ],

  "permissions": ["http://*/*", "https://*/*", "tabs", "storage"]
}
